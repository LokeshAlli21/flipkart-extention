{
    "version": 2,
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x",
            "memory": 1024,
            "maxDuration": 10,
            "env": {
                "PUPPETEER_SKIP_DOWNLOAD": "true"
            }
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
