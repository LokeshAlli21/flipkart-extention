{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["node_modules/chrome-aws-lambda/.bin/chromium"],
                "maxDuration": 10 
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "PUPPETEER_SKIP_DOWNLOAD": "true" 
    },
    "functions": {
        "api/*": {
            "runtime": "nodejs18.x",
            "memory": 3008 
        }
    }
}
