{
    "version": 2,
    "functions": {
        "index.js": {
            "runtime": "nodejs18.x",
            "memory": 1024,
            "maxDuration": 10,
            "env": {
                "PUPPETEER_SKIP_DOWNLOAD": "true"
            },
            "includeFiles": ["node_modules/chrome-aws-lambda/.bin/chromium"]
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}
